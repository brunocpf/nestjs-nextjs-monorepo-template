  "scripts": {
    "dev": "PORT=3001 npm run start:debug --",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/vitest --runInBand",
    "test:e2e": "vitest run --config ./test/vitest-e2e.config.ts",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
